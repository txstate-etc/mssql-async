{
  "name": "mssql-async",
  "version": "2.1.2",
  "description": "A wrapper for mssql for added convenience when working with async/await and inside docker containers.",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib-esm/index.js"
    },
    "./db": {
      "require": "./lib/db.js",
      "import": "./lib-esm/db.js"
    }
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "db": ["./lib/db.d.ts"]
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf ./lib && tsc",
    "mocha": "mocha -r ts-node/register test/**/*.ts --exit",
    "test": "./test.sh"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.8.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^9.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/mssql": "^7.1.3",
    "mssql": "^7.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/mssql-async.git"
  },
  "keywords": [
    "mssql",
    "await",
    "async"
  ],
  "author": "Nick Wing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/txstate-etc/mssql-async/issues"
  },
  "homepage": "https://github.com/txstate-etc/mssql-async#readme",
  "files": [
    "lib/**/*",
    "lib-esm/**/*"
  ]
}
